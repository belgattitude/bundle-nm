{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "target": "es6",
    "rootDir": "./",
    "module": "commonjs",
    "noEmit": true,
    "incremental": true,
    "noImplicitAny": false,
    "declaration": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "isolatedModules": true
  }
}
